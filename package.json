{
  "name": "cadensio-marketing",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "astro": "astro",
    "format:check": "oxfmt --check && prettier --check --no-error-on-unmatched-pattern \"**/*.astro\"",
    "format:fix": "oxfmt && prettier --write --no-error-on-unmatched-pattern \"**/*.astro\"",
    "lint": "oxlint",
    "pre-commit": "pnpm format:fix && pnpm lint && pnpm build",
    "commit": "cz"
  },
  "packageManager": "pnpm@10.26.2",
  "dependencies": {
    "@astrojs/react": "^4.4.2",
    "@astrojs/vercel": "^9.0.2",
    "@getbrevo/brevo": "^3.0.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.18",
    "@vercel/analytics": "^1.6.1",
    "astro": "^5.16.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "commitizen": "^4.3.1",
    "conventional-changelog-atom": "^5.0.0",
    "cz-customizable": "^7.5.1",
    "oxfmt": "^0.33.0",
    "oxlint": "^1.48.0",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "tw-animate-css": "^1.4.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
